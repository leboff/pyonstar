# OnStar Python API Credentials
# Copy this file to .env and fill in your values

# Required credentials
ONSTAR_USERNAME=your_email@example.com
ONSTAR_PASSWORD=your_password
ONSTAR_DEVICE_ID=your-device-id-uuid  # Must be a UUID4 (generate at https://www.uuidgenerator.net/version4)
ONSTAR_VIN=YOUR_VEHICLE_VIN

# Optional credentials
ONSTAR_PIN=1234
ONSTAR_TOTP_SECRET=YOUR_TOTP_SECRET

# Configuration
ONSTAR_DEBUG=true  # Set to false for production use 